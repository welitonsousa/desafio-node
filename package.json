{
  "name": "courses",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "predev": "dotenv -e .env drizzle-kit migrate",
    "dev": "node --env-file .env --watch src/server.ts",
    "-db": "===================================================================",
    "db-seed": "node --env-file .env ./database/seeds.ts",
    "db-generate": "drizzle-kit generate",
    "db-migrate": "drizzle-kit migrate",
    "db-studio": "drizzle-kit studio",
    "-db-test": "=============================================================",
    "db-migrate-test": "dotenv -e .env.test drizzle-kit migrate",
    "db-studio-test": "dotenv -e .env.test drizzle-kit studio",
    "db-drop-test": "node --env-file .env.test ./database/drop-database.ts",
    "-test": "================================================================",
    "pretest": "npm run db-drop-test && npm run db-migrate-test",
    "test": "dotenv -e .env.test vitest run --coverage",
    "posttest": "open ./coverage/index.html"
  },
  "dependencies": {
    "@fastify/swagger": "9.5.1",
    "@scalar/fastify-api-reference": "1.34.2",
    "argon2": "^0.44.0",
    "drizzle-orm": "0.44.4",
    "fastify": "5.5.0",
    "fastify-type-provider-zod": "5.0.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "8.16.3",
    "pino-pretty": "13.1.1",
    "zod": "4.0.17"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.1",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "0.31.4",
    "supertest": "^7.1.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}